                <account-manager>
                  <id>default-account-manager</id>
                </account-manager>
              </account-manager-claims-provider>
            </claims-value-provider>
            <claims-value-provider>
              <id>system-claims-provider</id>
              <system-information-claims-provider xmlns="https://curity.se/ns/ext-conf/system-information-claims-provider"/>
